{
  "name": "mdc-web-starter",
  "version": "0.1.0",
  "private": true,
  "main": "index.html",
  "scripts": {
    "start": "parcel ./index.html --open",
    "publish": "now dist/",
    "clean": "rm -rf dist/",
    "build": "npm run clean && parcel build ./index.html",
    "deploy": "npm run build && npm run publish",
    "dev:watch": "node ../../material-components-web/test/screenshot/run.js build --watch",
    "dev:serve": "parcel ./dev/index.html --open",
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "build:dev": "npm run clean && parcel build ./dev/index.html --no-minify",
    "deploy:dev": "npm run build:dev && npm run publish"
  },
  "dependencies": {
    "material-components-web": "^1.0.1",
    "cash-dom": "^3.1.0"
  },
  "devDependencies": {
    "sass": "^1.14.3",
    "parcel-bundler": "^1.10.3"
  }
}
